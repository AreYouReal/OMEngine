GL_VERTEX_SHADER

#version 300 es

uniform highp mat4 uModelViewM;
uniform highp mat4 uProjectionM;

layout(location = 0) in vec4 aPosition;

void main(){
    gl_Position = uProjectionM * uModelViewM * aPosition;
}

GL_FRAGMENT_SHADER

#version 300 es
precision mediump float;
out vec4 fragColor;

void main(){
    fragColor = vec4(1, 1, 1, 1);
}